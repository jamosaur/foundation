{
    "name": "jamosaur/foundation",
    "description": "Foundation for API's in Laravel",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Jamosaur\\Foundation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jamosaur\\Foundation\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "James Wallen-Jones",
            "email": "j.wallen.jones@googlemail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/routing": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "spatie/laravel-fractal": "^6.0",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0|^11.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0|^10",
        "mockery/mockery": "^1.4",
        "laravel/pint": "^1.27"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test:coverage": "vendor/bin/phpunit --coverage-html coverage"
    }
}
